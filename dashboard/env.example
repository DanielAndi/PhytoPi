# PhytoPi Dashboard Environment Variables
# Copy this file to .env.local for local development
# Do NOT commit .env.local to version control

# Supabase Configuration
# Local Development (default)
SUPABASE_URL=http://127.0.0.1:54321
SUPABASE_ANON_KEY=your-local-anon-key-here

# Production (uncomment and set for production builds)
# SUPABASE_URL=https://your-project.supabase.co
# SUPABASE_ANON_KEY=your-production-anon-key-here

# API Configuration
API_BASE_URL=http://127.0.0.1:54321

# App Configuration
APP_NAME=PhytoPi Dashboard
APP_VERSION=1.0.0

